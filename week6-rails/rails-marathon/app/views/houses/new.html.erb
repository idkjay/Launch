<div class="callout">
  <h1>New House Form</h1>
  <%= form_for(@house) do |f| %>
  <%= f.label :name, "Name" %><br />
  <%= f.text_field :name %>

  <%= f.label :source, "Source" %><br />
  <%= f.text_field :source %>

  <%= f.label :author, "Author" %><br />
  <%= f.text_field :author %>

  <%= f.label :motto, "Motto" %><br />
  <%= f.text_field :motto %>

  <%= f.submit "Add House" %>
  <% end %>

</div>
